}

@MastersThesis{Morvant2008,
  author   = {J. Morvant},
  title    = {{M}od\'elisation multi-\'echelle du comportement visco-plastique d'une plaque perfor\'ee},
  school   = {Rapport de stage Ecole Polytechnique},
  year     = {2008},
  abstract = {As part of crash simulations for aeronautic jointed areas, the stress
	and strain localization at the vincicity of the perforations is not
	correctly predicted and leads to a scale issue. Indeed, it is necessary
	to mesh acurately the riveted area in order to model correctly the
	localization phenomenon, however it leads to a computational cost.
	Thus a multiscale method seems better suited. The aim of this work
	is to develop a multiscale method to model the viscoplastic behaviour
	of a perforated plate. The first part of the report deals with the
	Transformation Field Analysis method, based on Dvorak formalism.
	In the second part, the return-mapping algorithm used in TFA is developped
	for plasticity only. Numerical results are compared with experiments
	and discussed. In the third part, the TFA algorithm for plasticity
	is evaluated on a perforated plate model, simulation results are
	compared with static experiments, and improvements for the method
	are proposed. The fourth part deals with two different viscoplastic
	constitutive laws and their implementation in the TFA method. Those
	laws are then tested with arbitrary material parameters to assess
	the influence of the strain rate on the TFA results.},
  file     = {Morvant2008.pdf:Morvant2008.pdf:PDF},
}

