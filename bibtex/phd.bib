  groups   = {Constitutive models},
  keywords = {Impact, Constitutive behaviour, Dynamic fracture, Elastic-viscoplastic material, Finite elements},
}

@Article{Borvik1999,
  author   = {T. Borvik and M. Langseth and O.S. Hopperstad and K.A. Malo},
  journal  = {International Journal of Impact Engineering},
  title    = {{B}allistic penetration of steel plates},
  year     = {1999},
  pages    = {855-886},
  volume   = {22},
  abstract = {This paper presents a research programme in progress where the main
	objective is to study the behaviour of Weldox 460 E steel plates
	impacted by blunt-nosed cylindrical projectiles in the lower ordnance
	velocity regime. A compressed gas gun is used to carry out high-precision
	tests, and a digital high-speed camera system is used to photograph
	the penetration process. A coupled constitutive model of viscoplasticity
	and ductile damage is formulated and implemented into the non-linear
	"nite element code LS-DYNA, and the material constants for the target
	plate are determined. The proposed model is applied in simulations
	of the plate penetration problem and the results are compared with
	test data. Good agreement between the numerical simulations and the
	experimental results is found for velocities well above the ballistic
	limit, while the ballistic limit itself is overestimated by approximately
	10% in the numerical simulations.},
  comment  = {Implementation of a coupled constitutive viscoplastic model and ductile
	damage in LS-DYNA},
  file     = {Borvik1999.pdf:Borvik1999.pdf:PDF},
  groups   = {Constitutive models},
  keywords = {Ballistic penetration, Experimental tests, Viscoplasticity, Damage, Numerical simulations},
}

