@Conference{Blanchard2006,
  author    = {S. Blanchard and B. Langrand and J. Fabis and A. Denquin},
  booktitle = {6th {I}nternational {F}{S}{W} {S}ymposium, {M}ontr\'eal, {C}anada},
  title     = {{A}rcan test and strain field measurement to study material behaviour in 6056{T}78 {F}{S}{W} specimens},
  year      = {2006},
  abstract  = {In this paper, Arcan tests are performed on base material and FSW
	specimens for comparison purposes, but also to assess the capability
	of strain field measurements technique to characterise non-linear
	behaviour of heterogeneous materials. Results show that the material
	behaviour observed thanks to measures on pure tensile Arcan test
	are similar to those observed on standard tensile specimens. However,
	pure shear results revealed a notable difference on deformation rupture
	characteristics. Those results illustrate the need to develop identification
	methods of material parameters based on heterogeneous strain fields
	in order to enrich the validity domain of those parameters.},
  file      = {Blanchard2006.pdf:Blanchard2006.pdf:PDF},
