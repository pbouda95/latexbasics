@Article{LeLouedec2013,
  author   = {Le Lou{\"e}dec, G. Le and Pierron, F. and Sutton, M. A. and Reynolds, A. P.},
  title    = {{I}dentification of the {L}ocal {E}lasto-{P}lastic {B}ehavior of {F}{S}{W} {W}elds {U}sing the {V}irtual {F}ields {M}ethod},
