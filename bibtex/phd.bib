@Article{Avril2008b,
  author   = {S. Avril and M. Bonnet and AS. Bretelle and M. Gr\'ediac and F. Hild and P. Ienny and F. Latourte and D. Lemosse and S. Pagano and E. Pagnacco and F. Pierron},
  journal  = {Experimental Mechanics},
  title    = {{O}verview of identification methods of mechanical parameters based on full-field measurements},
  year     = {2008},
  pages    = {381-402},
  volume   = {48},
  abstract = {This article reviews recently developed methods for constitutive parameter
	identification based on kinematic full-field measurements, namely
	the finite element model updating method (FEMU), the constitutive
	equation gap method (CEGM), the virtual fields method (VFM), the
	equilibrium gap method (EGM) and the reciprocity gap method (RGM).
	Their formulation and underlying principles are presented and discussed.
	These identification techniques are then applied to full-field experimental
	data obtained on four different experiments, namely (i) a tensile
	test, (ii) the Brazilian test, (iii) a shear-flexural test, and (iv)
	a biaxial test. Test (iv) features a non-uniform damage field, and
	hence non-uniform equivalent elastic properties, while tests (i),
	(ii) and (iii) deal with the identification of uniform anisotropic
	elastic properties. Tests (ii), (iii) and (iv) involve non-uniform
	strain fields in the region of interest.},
  comment  = {Review of the different techniques to identify elastic constitutive
	parameters: FEMU ------- FEMU-F Non updating method A quadratic form,
	which is built by comparing the nodal forces and the product between
	the stiffness matrix and the displacements (in a FE sense). This
	leads to a linear system (elastic case) where the constitutive parameters
	are the unknowns. FEMU-U Updating method A quadratic form is still
	minimized, but the latter is obtained by comparing the current displacement
	with those of a "reference" configuration (aka "well posed direct
	elasticity problem") CEGM (Constitutive equation gap method) ---------------------------------------------------------
	Updating method Minimize a quadratic form => Gap between a given
	stress field and the an another computed with the constitutive equations
	VFM ------ Non updating method :-) EGM (Equilibrium gap method) ------------------------------------------
	Updating method Minimize a quadratic form => Gap to a reference state
	(for the stiffness matrix for instance) Comes from a linear gap in
	the constitutive laws (damage)
	
	RGM (Reciprocity gap method) -----------------------------------------
	Updating method Based on Maxwell Betti theorem. A functional has
	to be equal to zero.},
  file     = {Avril2008b.pdf:Avril2008b.pdf:PDF},
  groups   = {Parameters identification},
}

@Article{Avril2010,
  author   = {Stéphane Avril and Pierre Feissel and Fabrice Pierron and Pierre Villon},
  title    = {{C}omparison of two approaches for differentiating full-field data in solid mechanics},
  journal  = {Measurement Science and Technology},
  year     = {2010},
  volume   = {21},
  number   = {1},
  pages    = {015703},
  abstract = {In this study, the issue of reconstructing the gradients of noisy
	full-field data is addressed within the framework of solid mechanics.
	Two approaches are considered, a global one based on finite element
	approximation (FEA) and a local one based on diffuse approximation
	(DA). For both approaches, it is proposed to monitor locally the
	filtering effect in order to adapt the uncertainty to the local signal
	to noise ratio. Both approaches are applied to a case study which
	is commonly considered as difficult in solid mechanics (open-hole
	tensile test on a composite laminate). Both DA and FEA are successful
	for detecting local subsurface damage from the measured noisy displacement
	fields. Indications are also provided about the compared performances
	of DA and FEA. It is shown that DA is more robust, but the downside
	is that it is also more CPU time consuming.},
  file     = {Avril2010.pdf:Avril2010.pdf:PDF},
  url      = {http://stacks.iop.org/0957-0233/21/i=1/a=015703},
}

