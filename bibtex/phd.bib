@Article{Peirs2011,
  author   = {J. Peirs and P. Verleysen and J. Degrieck},
  title    = {{E}xperimental study of the influence of strain rate on fracture of ti6{A}l4{V}},
  journal  = {Procedia Engineering},
  year     = {2011},
  volume   = {10},
  pages    = {2336-2341},
  abstract = {The effect of the strain rate, stress triaxiality and temperature
	on the fracture properties of Ti6Al4V is studied. First, static and
	dynamic in-plane tensile, shear and plain-strain tests are carried
	out. Classical measurements are combined with full field strain measurement
	by means of digital image correlation to assess the local strain
	during deformation. In addition, finite element simulations are used
	to gain more insight into the specimen behavior. Second, the fracture
	surface and the surrounding material are studied with electron microscopy.
	Thereby, void density and distribution are measured. It is found
	that the void density decreases very fast with increasing distance
	from the fracture while a more gradual decrease of the average void
	size is observed. Besides on the amount of strain, the void density
	depends on the stress triaxiality. Furthermore, the stress triaxiality
	during plastic deformation of the specimen is important. In contrast,
	the effect of the strain rate on the fracture properties is rather
	limited. Except for high strain rate shear tests where the formation
	of an adiabatic shear band precedes ductile fracture.},
  file     = {Peirs2011a.pdf:Peirs2011a.pdf:PDF},
  keywords = {Ti6Al4V, High strain rate, Hopkinson, Ductile fracture, Microscopy},
}

