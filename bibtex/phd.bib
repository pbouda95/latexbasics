@Article{Besnard2006,
  author   = {Besnard, G. and Hild, F. and Roux, S.},
  title    = {``{F}inite-{E}lement'' {D}isplacement {F}ields {A}nalysis from {D}igital {I}mages: {A}pplication to {P}ortevin--{L}e {C}h{\^a}telier {B}ands},
