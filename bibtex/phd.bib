@Conference{Shearer2006,
  author    = {C. M. Shearer and C. E. S. Cesnik},
  title     = {{M}odified {G}eneralized-alpha {M}ethod for {I}ntegrating {G}overning {E}quations of {V}ery {F}lexible {A}ircraft},
  booktitle = {47th {A}{I}{A}{A}/{A}{S}{M}{E}/{A}{S}{C}{E}/{A}{H}{S}/{A}{S}{C} {S}tructures, {S}tructural {D}ynamics, and {M}aterials {C}onference},
  year      = {2006},
  abstract  = {This paper focuses on the time integration of the nonlinear EOM associated
	with a very flexible aircraft in fight. Various integration methods
	exist for linear structural dynamics problems. However, a review
	of the literature indicates little material associated with the integration
	of nonlinear structural EOM of relatively large order. Moreover,
	for the problem of simulation of very flexible aircraft, a combination
	of flight dynamics and aeroelastic degrees of freedom must be solved.
	A modified first and second order Generalized-alpha Method along
	with an implicit sub-iteration scheme were developed. It has shown
	good agreement with predictor/corrector integration schemes for a
	reduced set of linear EOM. The method is also seen to be numerically
	stable when compared to non-dissipative time marching integration
	schemes and requires less computational time compared to predictor/corrector
	methods for the full set of nonlinear EOM.},
  file      = {Shearer2006.pdf:Shearer2006.pdf:PDF},
  keywords  = {Modified Generalized-alpha Method, Time integration, Flexible Aircraft, EOM},
}

@Article{Siarry1997b,
  author   = {P. Siarry and G. Berthiau},
  title    = {{F}itting of tabu search to optimize functions of continuous variables},
  journal  = {International Journal for Numerical Methods in Engineering},
  year     = {1997},
  volume   = {40(13)},
  pages    = {2449-2457},
  abstract = {Tabu Search (TS) is a stochastic global optimization procedure which
	proved effcient to solve various combinatorial optimization problems.
	However, very few works deal with its application to global minimization
	of functions depending on continuous variables. The aim of this paper
	is to propose an adaptation of TS to the optimization of continuous
	functions, and to study the inßuence of the main algorithm parameters
	on the convergence towards the optimum. In particular, the application
	of TS to function optimization involves the definition of the current
	solution neighbourhood and the management of the tabu list. The effciency
	of TS applied to continuous global optimization has been tested in
	detail by using classical multimodal functions for which minima are
	known.},
  file     = {Siarry1997b.pdf:Siarry1997b.pdf:PDF},
  keywords = {tabu search, global optimization, continuous variables},
}

@Article{Siarry1997a,
  author   = {P. Siarry and G. Berthiau and F. Durbin and J. Haussy},
  title    = {{E}nhanced simulated annealing for globally minimizing functions of many continuous variables},
  journal  = {ACM Transactions on Mathematical Software},
  year     = {1997},
  volume   = {23(2)},
  pages    = {209-228},
  abstract = {A new global optimization algorithm for functions of many continuous
	variables is presented, derived from the basic Simulated Annealing
	method. Our main contribution lies in dealing with high-dimensionality
	minimization problems, which are often difficult to solve by all
	known minimization methods with or without gradient. In this article
	we take a special interest in the variables discretization issue.
	We also develop and implement several complementary stopping criteria.
	The original Metropolis iterative random search, which takes place
	in a Euclidean space Rn, is replaced by another similar exploration,
	performed within a succession of Euclidean spaces Rp, with p << n.
	This Enhanced Simulated Annealing (ESA) algorithm was validated first
	on classical highly multimodal functions of 2 to 100 variables. We
	obtained significant reductions in the number of function evaluations
	compared to six other global optimization algorithms, selected according
	to previously published computational results for the same set of
	test functions. In most cases, ESA was able to closely approximate
	known global optima. The reduced ESA computational cost helped us
	to refine further the obtained global results, through the use of
	some local search. We have used this new minimizing procedure to
	solve complex circuit design problems, for which the objective function
	evaluation can be exceedingly costly.},
  file     = {Siarry1997a.pdf:Siarry1997a.pdf:PDF},
  keywords = {Optimization, Nonlinear programming, Probability and Statistics, Probabilistic algorithms, Mathematical Software, certification and testing, Algorithms, Performance, Global optimization, stochastic optimization, test function},
}

